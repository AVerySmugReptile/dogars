<h2>Champs</h2>
<div>
	<div class="champs">
		{{#champs}}
			<div class="champ">
				<img src="http://play.pokemonshowdown.com/sprites/trainers/{{avatar}}.png"/>
				<div class="cdata">
					<div class="cname">{{{name}}}</div>
					<div class="ctrip">{{trip}}</div>
				</div>
				<div class="winlosses">W/L: <i class="green">{{wins}}</i>/<i class="red">{{loses}}</i></div>
				{{#elo}}<div class="elo"><div>{{elo}}</div></div>{{/elo}}
			</div>
		{{/champs}}
	</div>
</div>
