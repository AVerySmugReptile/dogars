<div class="set-info">
	<h3><a href="/set/{{id}}">{{#name}}{{name}}{{/name}}{{^name}}{{species}}{{/name}}</a></h3>
	<div><img class="set-image {{#cust}}custom{{/cust}}" data-id="{{id}}" src="{{img_url}}"></div>
	<div class="info">
		{{#description}}
			<label>Description: </label>
			<div class="description">{{{description_html}}}</div>
		{{/description}}
		<label>Date added: </label>
		<div >{{date}}</div>
		{{#creator}}
			<label>Creator: </label>
			<div>{{creator}}</div>
		{{/creator}}
		{{#hash}}
			<label>Tripcode: </label>
			<div>{{hash}}</div>
		{{/hash}}
		<label>Format: </label>
		<div>{{format}}</div>
	</div>
</div>
