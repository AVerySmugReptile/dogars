<h3><a href="/set/{{id}}">{{#name}}{{name}}{{/name}}{{^name}}{{species}}{{/name}}</a></h3>
<img class="set-image {{#cust}}custom{{/cust}}" data-id="{{id}}" src="{{img_url}}"><br>
<div class="info">
	<div class="row">{{#description}}<label>Description: </label><div class="field description">{{{description_html}}}</div><br>{{/description}}</div>
	<div class="row"><label>Date added: </label><div class=field>{{date}}</div><br></div>
	<div class="row">{{#creator}}<label>Creator: </label><div class=field>{{creator}}</div><br>{{/creator}}</div>
	<div class="row">{{#hash}}<label>Tripcode: </label><div class=field>{{hash}}</div><br>{{/hash}}</div>
	<div class="row"><label>Format: </label><div class=field>{{format}}</div><br></div>
</div>
